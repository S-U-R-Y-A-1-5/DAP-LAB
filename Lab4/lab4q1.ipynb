   "source": [
    "def get_ticket_price(age):\n",
    "    if age < 5:\n",
    "        return \"Free\"\n",
    "    elif age <= 18:\n",
    "        return \"₹100\"\n",
    "    elif age <= 60:\n",
    "        return \"₹200\"\n",
    "    else:\n",
    "        return \"₹150\"\n",
    "age = int(input(\"Enter your age: \"))\n",
    "print(f\"Your ticket price is: {get_ticket_price(age)}\")\n"
   ]
